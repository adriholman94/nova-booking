<div class="content-heading">
  <div> Mis Propiedades</div>

  <div class="ml-auto">
    <%= link_to new_estate_path do %>
      <button class="btn btn btn-info" title="Nueva propiedad">
        <i class="fas fa-plus text-light fa-1x"></i>
      </button>
    <% end %>
  </div>
</div>

<div id="results">
